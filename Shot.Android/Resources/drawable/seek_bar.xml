<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:id="@android:id/background">
		<shape android:shape="line">
			<stroke android:color="@color/material_grey_600" android:width="4dp" />
		</shape>
	</item>
	<item android:id="@android:id/secondaryProgress">
		<clip>
			<shape android:shape="line">
				<stroke android:color="@android:color/holo_red_dark" android:width="0dp" />
			</shape>
		</clip>
	</item>
	<item android:id="@android:id/progress">
		<clip>
			<shape android:shape="line">
				<stroke android:color="@android:color/holo_red_dark" android:width="4dp" />
			</shape>
		</clip>
	</item>
</layer-list>